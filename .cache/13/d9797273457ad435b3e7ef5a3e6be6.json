n: 0.5s cubic-bezier(0.175, 0.885, 0.32, 1.275) background;\n\n  &:hover,\n  &:focus {\n    color: #222;\n    background: $primary-color;\n  }\n}\n\ninput,\ntextarea {\n  background-color: rgba(white, 0.05);\n  padding: 10px 16px;\n  border-radius: 20px;\n  border: 2px solid #9893d8;\n  color: #f2f2f2;\n}\n\nh1,\nh2,\nh3 {\n  color: #dddbff;\n  font-weight: normal;\n}\n\np,\na,\nli {\n  color: #9893d8;\n}\n\nh1,\nh3 {\n  font-size: 1.2em;\n}\n\nh2 {\n  font-size: 1.6em;\n}\n\nh4 {\n  font-size: 1.1em;\n}\n\n.leading {\n  font-size: 1.1em;\n}\n","scss/_media-queries.scss":"/* small tablet styles */\n@media screen and (min-width: 620px) {\n}\n/* large tablet & laptop styles */\n\n/* Desktop tablet styles */\n","scss/_layout.scss":"// mobile first layout\n.grid {\n  display: grid;\n  gap: 10px;\n  grid-template-columns: repeat(8, 1fr);\n  padding: 0 20px 20px 20px;\n\n  .welcome-text {\n    grid-column: 1/6;\n\n    a {\n      margin: 20px 0;\n    }\n  }\n\n  .welcome-img {\n    grid-column: 6/9;\n\n    img {\n      width: 200%;\n    }\n  }\n}\n\n.site-nav {\n  margin: 0 20px;\n\n  ul {\n    display: none;\n  }\n\n  h1 {\n    grid-column: 1/4;\n    grid-row: 1;\n    padding-bottom: 10px;\n    border-bottom: 3px solid $primary-color;\n  }\n}\n\n.portfolio {\n  background: url(../assets/square_bg.png) no-repeat bottom/cover;\n  padding: 30px 20px;\n\n  h3 {\n    margin-bottom: 0;\n    text-align: center;\n  }\n}\n\n.projects {\n  a {\n    display: flex;\n    grid-column: 1/9;\n    margin: 20px 0;\n  }\n\n  a * {\n    margin: 0 20px;\n    max-width: 40%;\n    align-self: center;\n  }\n\n  h4 {\n    color: #9994f7;\n  }\n}\n\n.skills {\n  padding: 30px 20px;\n  background: #07083e;\n\n  h3 {\n    text-align: center;\n  }\n\n  ul {\n    margin: 40px 0;\n\n    li {\n      padding: 10px;\n      background: rgba(black, 0.2);\n      grid-column: span 4;\n      text-align: center;\n    }\n  }\n\n  img {\n    height: 80px;\n    margin: 10px auto;\n  }\n}\n"},"lineCount":null}},"er